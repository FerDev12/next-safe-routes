{
  "name": "next-safe-routes",
  "version": "0.0.4",
  "description": "Type-Safe navigation for Next.js.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsup",
    "dev": "tsup --watch"
  },
  "bin": {
    "generate-routes": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "import": "./dist/plugin/index.js",
      "types": "./dist/plugin/index.d.ts",
      "require": "./dist/plugin/index.js"
    },
    "./navigation": {
      "import": "./dist/navigation.ts",
      "types": "./dist/navigation.d.ts",
      "export": "./dist/navigation.ts"
    },
    "./create-get-safe-route": {
      "import": "./dist/navigation/create-get-safe-route.ts",
      "types": "./dist/navigation/create-get-safe-route.d.ts",
      "export": "./dist/navigation/create-get-safe-route.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "next",
    "react",
    "routes",
    "routing",
    "navigation",
    "type-safe",
    "safe routes"
  ],
  "author": "FerDev12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FerDev12/next-safe-routes"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@types/eslint": "^8",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/yargs": "^17.0.33",
    "eslint": "^8",
    "next": "^14.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
